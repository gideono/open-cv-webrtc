  "main": "index.js",
  "scripts": {
    "start": "npm run build && NODE_ENV=production node ./src/backend/dist/main.js ",
    "local-backend": "npm run webpack && nodemon ./src/backend/main.js --exec babel-node --presets=es2015,stage-3",
    "local-frontend": "webpack-dev-server",
    "build": "rm -rf ./src/backend/dist && babel ./src/backend --out-dir ./src/backend/dist --ignore=test,resources --copy-files --presets es2015,stage-3 && NODE_ENV=production npm run webpack",
    "wc": "nodemon ./src/backend/web-camera.js --exec babel-node --presets=es2015,stage-3",
    "test": " mocha --compilers js:babel-register --recursive ./src/**/*.spec.js",
    "webpack": "webpack",
    "w": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gideono/open-cv-webrtc.git"
  },
